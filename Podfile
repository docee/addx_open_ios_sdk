  pod 'MJRefresh' , '~>3.2'
  pod 'SDCAlertView'
  pod 'IQKeyboardManager'
  
  pod 'ZendeskAnswerBotSDK'#, '~> 2.1.2'
  pod 'ZendeskSupportSDK'#, :path => 'ZendeskSDK'
  pod 'ZendeskChatSDK'#, :path => 'ZendeskChatSDK'
  
  pod 'ADVideoMessageManager', :path => 'ADVideoMessageManager'
  pod 'lottie-ios', :path => 'lottie-ios'
  pod 'ADDXWebRTC', :path => 'ADDXWebRTC'
  pod 'A4xSDK', :path => 'A4xSDK'
  pod 'LFLiveKit', :path => 'LFLiveKit'
